{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "$id": "https://example.com/schemas/GenericMapper.json",
  "title": "GenericMapper",
  "type": "object",
  "required": [
    "source",
    "target",
    "generic_combinations"
  ],
  "properties": {
    "source": {
      "type": "array",
      "items": {
        "$ref": "DataTypeIdentifier.json"
      }
    },
    "target": {
      "type": "string"
    },
    "generic_combinations": {
      "type": "array",
      "items": {
        "enum": [
          "AND",
          "OR"
        ]
      }
    }
  }
}
